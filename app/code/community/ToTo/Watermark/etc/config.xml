<config>
    <modules>
        <ToTo_Watermark>
            <version>0.1.0</version>
        </ToTo_Watermark>
    </modules>
    <frontend>
        <routers>
            <watermark>
                <use>standard</use>
                <args>
                    <module>ToTo_Watermark</module>
                    <frontName>watermark</frontName>
                </args>
            </watermark>
        </routers>
        <layout>
            <updates>
                <watermark>
                    <file>watermark.xml</file>
                </watermark>
            </updates>
        </layout>
    </frontend>
</config>